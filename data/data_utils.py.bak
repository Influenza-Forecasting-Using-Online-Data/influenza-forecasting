import pandas as pd
import numpy as np

def get_datetime_data():
    df = pd.read_csv("./year_and_week_data_frame.csv")
    datetime_df = pd.DataFrame({"year week": pd.to_datetime(df["year"].astype(str) + " " + df["week"].astype(str) + " 1",
                                                         format="%Y %U %w")})
    return pd.concat([datetime_df, df], axis=1)